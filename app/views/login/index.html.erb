<h1>Login</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for :user, url: login_path do |form|%>
      <%= form.label :email%>
      <%= form.text_field :email %>
      <%= form.label :password%>
      <%= form.password_field :password %>
      <%= form.submit "Login"%>
    <% end %>
  </div>
</div>